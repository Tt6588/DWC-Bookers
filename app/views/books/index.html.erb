<h1>Books</h1>

<table>
  <thead>
    <tr>
      <th>Title</th> <th>Body</th> <th colspan="3"></th>
    </tr>
  </thead>
  
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td>title</td> 
        <td>body</td> 
        <td>Show</td> 
        <td>Edit</td> 
        <td>Destroy</td>
      </tr>
    <% end %>
  </tbody>
</table>


<h1>New book</h1>

<%= form_with model: @book, url:"/books", local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_fild :title %>
  <h4>Body</h4>
  <%=f.text_area :body %>
  <%=f.submit 'Create Book' %>
<% end %>